{
  "name": "tockler",
  "productName": "Tockler",
  "version": "2.2.0",
  "license": "MIT",
  "author": "Maigo Erit <maigo.erit@gmail.com>",
  "main": "src/server/main.electron.js",
  "scripts": {
    "clean": "rimraf node_modules doc dist coverage out && npm cache clean",
    "clean-generated": "rimraf dist doc coverage out",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "e2e": "npm run package-e2e && protractor",
    "e2e-live": "npm run package-e2e && protractor --elementExplorer",
    "package-e2e": "node scripts/packager.js --platform=current",
    "pretest": "npm run lint",
    "test": "standard && karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run build && npm run test && npm run e2e",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "start": "rimraf dist && webpack --progress --profile --bail && electron .",
    "start-watch": "set NODE_ENV=development && rimraf dist && webpack --progress --profile --bail --watch",
    "postinstall": "electron-rebuild && npm run webdriver-update && typings install"

  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "core-js": "^2.4.1",
    "debug": "^2.2.0",
    "electron-connect": "^0.5.0",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "standard": "^8.0.0",
    "zone.js": "0.6.21",
    "d3": "^4.2.7",
    "electron-context-menu": "^0.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "es-module-loader": "^1.0.0",
    "es6-promise": "^4.0.5",
    "lodash": "^4.16.4",
    "menubar": "^5.1.0",
    "moment": "^2.15.1",
    "multiline": "^1.0.2",
    "ng2-translate": "2.5.0",
    "node-notifier": "^4.6.1",
    "os-locale": "^1.4.0",
    "q": "^1.4.1",
    "randomcolor": "^0.4.4",
    "sequelize": "^3.24.3",
    "sqlite3": "^3.1.6",
    "auto-launch": "^5.0.1",
    "bunyan": "^1.8.1",
    "compare-version": "^0.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "codelyzer": "0.0.25",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.25.0",
    "electron-packager": "^8.0.0",
    "electron-connect": "0.6.0",
    "electron": "1.4.3",
    "electron-rebuild": "1.2.1",
    "electron-debug": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.3",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-electron-launcher": "^0.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "null-loader": "0.1.1",
    "on-build-webpack": "^0.1.0",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.13.0",
    "protractor": "^4.0.0",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.1",
    "sass-loader": "^4.0.0",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.0",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.4.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.2"
  },
  "standard": {
    "globals": [
      "jasmine"
    ],
    "ignore": [
      "src/*"
    ]
  },
  "build": {
    "npmRebuild": true,
    "appId": "ee.trimatech.Tockler",
    "iconUrl": "shared/img/icon/timetracker_icon.ico",
    "asar": false,
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "shared/img/icon/timetracker_icon.icns",
      "icon-size": 40,
      "contents": [
        {
          "x": 438,
          "y": 144,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 144,
          "type": "file"
        }
      ]
    },
    "win": {
      "iconUrl": "https://raw.githubusercontent.com/MayGo/tockler/master/build/icon.ico",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "deb"
    }
  }
}
